{
	context:[{
        name:"numbers",
        type:"numberList",
        values:[1,2,3,4,5]
    },{        
		name:"sampler",
        type:"listSampler",
        source:{
            type:"reference",
            refersTo:"numbers"
        }
	},{
        name:"sequence",
        type:"listSequence",
        source:{
            type:"reference",
            refersTo:"numbers"
        }
    },
    {
		name:"randomOutput",
		type:"iterator",
		sources:[{
			type:"singleValue",
			source:{
				type:"reference",
				refersTo:"sampler"
			}
		}]
	},{
		name:"sequenceOutput",
		type:"iterator",
		sources:[{
			type:"singleValue",
			source:{
				type:"reference",
				refersTo:"sequence"
			}
		}]
	}],
	output:[{
        type:"fileOutput",
        filename:"output1.txt",
        targets:[{
            type:"times",
            count:10,
            targets:[{        
                type:"formatString",
                format:"{0}",
                source:{
                    type:"reference",
                    refersTo:"randomOutput"
                }
            }] 
        }, {
            type:"literal",
            value:"-----------------------------------------"
        },{
            type:"times",
            count:10,
            targets:[{
                type:"formatString",
                format:"{0}",
                source:{
                    type:"reference",
                    refersTo: "sequenceOutput"
                }
            }]
        }]
	}]
}